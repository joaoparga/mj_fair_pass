  keywords = {Environmental equity,Sustainable transportation},
  file = {/Users/joaoparga/Zotero/storage/NSCM5CA7/Feitelson - 2002 - Introducing environmental equity dimensions into t.pdf;/Users/joaoparga/Zotero/storage/GPTMGS82/S136192090100013X.html}
}

