  keywords = {Accessibility,Conceptual model,Must read,Only AIC (finish later) {$\warning$}️,Perceived accessibility,Policy evaluation,Transport planning},
  file = {/Users/joaoparga/Zotero/storage/CAHBX5JV/Pot et al. - 2021 - Perceived accessibility What it is and why it dif.pdf}
}

@article{pozoukidou15MinuteCityDecomposing2021,
  title = {15-{{Minute City}}: {{Decomposing}} the {{New Urban Planning Eutopia}}},
