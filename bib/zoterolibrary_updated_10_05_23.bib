@article{stevensonLandUseTransport2016,
  title = {Land Use, Transport, and Population Health: Estimating the Health Benefits of Compact Cities},
  shorttitle = {Land Use, Transport, and Population Health},
  author = {Stevenson, Mark and Thompson, Jason and {de S{\'a}}, Thiago H{\'e}rick and Ewing, Reid and Mohan, Dinesh and McClure, Rod and Roberts, Ian and Tiwari, Geetam and {Giles-Corti}, Billie and Sun, Xiaoduan and Wallace, Mark and Woodcock, James},
