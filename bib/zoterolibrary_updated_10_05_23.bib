@article{tingTamingHumanSubjects2023,
  title = {Taming {{Human Subjects}}: {{Researchers}}' {{Strategies}} for {{Coping}} with {{Vagaries}} in {{Social Science Experiments}}},
  shorttitle = {Taming {{Human Subjects}}},
  author = {Ting, Carol and Montgomery, Martin},
  year = {2023},
