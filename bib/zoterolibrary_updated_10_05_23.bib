@article{rothmanSixPersistentResearch2014,
  title = {Six {{Persistent Research Misconceptions}}},
  author = {Rothman, Kenneth J.},
  year = {2014},
  month = jul,
  journal = {Journal of General Internal Medicine},
  volume = {29},
  number = {7},
