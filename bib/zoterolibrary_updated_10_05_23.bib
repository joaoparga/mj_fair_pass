@article{mccannUrbanPolicyMobilities2011,
  title = {Urban {{Policy Mobilities}} and {{Global Circuits}} of {{Knowledge}}: {{Toward}} a {{Research Agenda}}},
  shorttitle = {Urban {{Policy Mobilities}} and {{Global Circuits}} of {{Knowledge}}},
  author = {McCann, Eugene},
  year = {2011},
  journal = {Annals of the Association of American Geographers},
  volume = {101},
