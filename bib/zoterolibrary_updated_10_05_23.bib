@article{graells-garridoCityCitiesMeasuring2021,
  title = {A City of Cities: {{Measuring}} How 15-Minutes Urban Accessibility Shapes Human Mobility in {{Barcelona}}},
  shorttitle = {A City of Cities},
  author = {{Graells-Garrido}, Eduardo and {Serra-Burriel}, Feliu and Rowe, Francisco and Cucchietti, Fernando M. and Reyes, Patricio},
