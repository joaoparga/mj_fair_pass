@article{boisjolyAccessibilityMeasurementsSao2020,
  title = {Accessibility Measurements in {{S\~ao Paulo}}, {{Rio}} de {{Janeiro}}, {{Curitiba}} and {{Recife}}, {{Brazil}}},
