@article{arcuryAccessTransportationHealth2005a,
  title = {Access to {{Transportation}} and {{Health Care Utilization}} in a {{Rural Region}}},
  author = {Arcury, Thomas A. and Preisser, John S. and Gesler, Wilbert M. and Powers, James M.},
  year = {2005},
  journal = {The Journal of Rural Health},
  volume = {21},
