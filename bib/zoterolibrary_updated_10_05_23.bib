@article{jafariResidentialDensity20minute2023,
  title = {Residential Density and 20-Minute Neighbourhoods: {{A}} Multi-Neighbourhood Destination Location Optimisation Approach},
  shorttitle = {Residential Density and 20-Minute Neighbourhoods},
  author = {Jafari, Afshin and Singh, Dhirendra and {Giles-Corti}, Billie},
  year = {2023},
